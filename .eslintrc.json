{
    "extends": "airbnb",
    "installedESLint": true,
    "plugins": [
        "react"
    ],
    "rules": {
        "no-console": 0,
        "no-restricted-syntax": 0,
        "prefer-const": 0,
        "no-param-reassign": 0
    },
    "globals": {
      "Game": true,
      "Memory": true,
      "WORK": true,
      "CARRY": true,
      "MOVE": true,
      "ATTACK": true,
      "RANGED_ATTACK": true,
      "HEAL": true,
      "CLAIM": true,
      "TOUGH": true,
      "FIND_STRUCTURES": true,
      "FIND_SOURCES": true,
      "STRUCTURE_SPAWN": true,
      "STRUCTURE_TOWER": true,
      "ERR_NOT_IN_RANGE": true,
      "STRUCTURE_EXTENSION": true,
      "RESOURCE_ENERGY": true
    }
}
